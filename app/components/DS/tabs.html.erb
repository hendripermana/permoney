<%= tag.div data: {
  controller: "DS--tabs",
  testid: testid,
  "DS--tabs-session-key-value": session_key,
  "DS--tabs-url-param-key-value": url_param_key,
  "DS--tabs-nav-btn-active-class": active_btn_classes,
  "DS--tabs-nav-btn-inactive-class": inactive_btn_classes
} do %>
  <% if unstyled? %>
    <%= content %>
  <% else %>
    <%= nav %>

    <% panels.each do |panel| %>
      <%= panel %>
    <% end %>
  <% end %>
<% end %>
